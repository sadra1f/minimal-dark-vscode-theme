    "breadcrumbPicker.background": "#252526",

    "sideBar.background": "#1E1E1E",
    "sideBar.border": "#1E1E1E",
    "sideBar.foreground": "#BBBBBB",
    "sideBarTitle.foreground": "#BBBBBB",
    "sideBarSectionHeader.background": "#1E1E1E",
    "sideBarSectionHeader.foreground": "#DCDFE4",
    "sideBarSectionHeader.border": "#1E1E1E",
    "sideBar.dropBackground": "#ffffff16",

    "list.inactiveSelectionBackground": "#252526",
    "list.activeSelectionBackground": "#e3f2ff22",
    "list.activeSelectionForeground": "#FFF",
    "list.hoverBackground": "#ffffff14",
    "list.hoverForeground": "#DCDFE4",
