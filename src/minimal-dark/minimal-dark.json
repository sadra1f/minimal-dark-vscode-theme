    "editorGroup.dropBackground": "#ffffff16",
    "editorGroupHeader.tabsBackground": "#252526",
    "editorGroupHeader.noTabsBackground": "#ffffff16",
    "sash.hoverBorder": "#383B3D",
    "activityBar.background": "#252526",
    "activityBar.foreground": "#FFF",
    "activityBar.border": "#252526",
    "activityBar.dropBorder": "#ffffff40",
